{
  "name": "datalayerjs",
  "version": "0.0.1",
  "description": "An open-source datalayer, tagmanager and frontend middleware, sitting between the client and any third parties",
  "main": "dist/datalayer.cjs.js",
  "module": "dist/datalayer.esm.js",
  "scripts": {
    "test:spec": "jest --coverage",
    "test:lint": "eslint . --quiet",
    "test": "npm run test:lint && npm run test:spec",
    "build:cjs": "rollup src/datalayer.js --o dist/datalayer.cjs.js -f cjs",
    "build:esm": "rollup src/datalayer.js --o dist/datalayer.esm.js -f es",
    "build:umd": "rollup src/datalayer.js --o dist/datalayer.umd.js -f umd --name \"datalayerjs\"",
    "build": "npm run build:cjs && npm run build:esm && npm run build:umd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://ricopfaus@bitbucket.org/ricopfaus/datalayerjs.git"
  },
  "keywords": [
    "tagmanager",
    "datalayer",
    "affiliate",
    "attribution"
  ],
  "author": "Rico Pfaus",
  "license": "MIT",
  "homepage": "https://bitbucket.org/ricopfaus/datalayerjs#readme",
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "babel-plugin-dynamic-import-node": "^1.1.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "babelrc-rollup": "^3.0.0",
    "eslint": "^4.8.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-plugin-import": "^2.7.0",
    "jest": "^22.4.3",
    "jest-environment-jsdom": "^22.4.3",
    "jest-environment-jsdom-global": "^1.0.3",
    "jsdom": "^11.3.0",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2"
  },
  "dependencies": {
    "promise": "^8.0.1"
  },
  "jest": {
    "testEnvironment": "jest-environment-jsdom-global"
  }
}
